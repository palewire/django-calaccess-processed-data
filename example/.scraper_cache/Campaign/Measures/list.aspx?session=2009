			<script language='javascript' type='text/javascript'>
			function trapEnter(btn)
			{
				if (document.all)
				{
					if (event.keyCode == 13)
					{ 
						event.returnValue=false;
						event.cancel = true;
						btn.click();
					} 
				} 
			}
			</script>

</form>
									</td>
								</tr>
							</table>
						</form>
					</body>
				</html>
			